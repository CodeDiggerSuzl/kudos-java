<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.UserInfoDao">
    <select id="findByAge" resultType="dos.UserInfoDo">
        select * from user_info where age = #{age}
